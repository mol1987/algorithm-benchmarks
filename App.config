<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--
    TimeUnit determines what time unit is displayed in the summary table
    Choices:
      - Nanosecond
      - Microsecond
      - Millisecond
      - Second
      - Minute
      - Hour
      - Day
    Default value: Microsecond
    -->
    <add key="TimeUnit" value="Millisecond"/>
    <!--
    SizeUnit determines what size unit is displayed in the summary table
    Choices:
      - B           (Byte)
      - KB          (KiloByte)
      - MB          (MegaByte)
      - GB          (GigaByte)
      - TB          (TerraByte
    Default value: KB
    -->
    <add key="SizeUnit" value="KB"/>
    <!--
    DataSize is a list of values that determines what sizes of data the algorithms
    should bennchmark on. So a value of "10, 100" would mean that the benchmark would 
    first run all algorithms with a list of integers with 10 elements, and then a list of 100
    Choices:
      - [values]              (ex. 100)
      - reverse=[value]       (ex. reverse=100)
      - last_lowest=[value]   (ex. last_lowest=100)
    -->
    <add key="DataSizes" value="100, last_lowest=100"/>
  </appSettings>
</configuration>
